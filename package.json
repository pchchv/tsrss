    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.7",
    "eslint": "^5.16.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^3.0.1",
    "redux": "^5.0.1",
    "rss-parser": "^3.13.0",
    "typescript": "^4.3.5",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10",
    "@types/xml2js": "^0",
    "autoprefixer": "^10.4.20",
    "ts-node": "^10.9.2"
  }
